  "scripts": {
    "start": "SET NODE_ENV=development && tsc && gulp clean --color && gulp copy-images --color && webpack --env=development -d && concurrently \"webpack-dev-server --color  -d --watch \" \"dotnet watch run\" ",
    "debug": "SET NODE_ENV=development && tsc && gulp clean --color && gulp copy-images --color && webpack --env=development -d && webpack-dev-server  --color  -d --watch",
    "prod": "SET NODE_ENV=production && gulp init --color && concurrently \"webpack --env=production -p --color\" \"dotnet watch run\" ",
    "aot": "SET NODE_ENV=production && gulp init --color && ngc -p ./tsconfig-aot.json && concurrently \"webpack --env=aot -p --color\" ",
    "ngc": "ngc -p ./tsconfig-aot.json",
    "devServer": "webpack --env=development -d && webpack-dev-server --env=development --color --watch ",
    "tsc": "tsc",
    "tsc:w": "tsc -w",
    "clean-dist": "rimraf wwwroot/aot && rimraf wwwroot/dist && rimraf wwwroot/index.html",
    "cleanup": "rimraf node_modules && rimraf typings"
  },
